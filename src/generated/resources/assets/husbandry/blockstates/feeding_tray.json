{
  "multipart": [
    {
      "apply": {
        "model": "husbandry:block/feeding_tray"
      }
    },
    {
      "when": {
        "fill": "1"
      },
      "apply": {
        "model": "husbandry:block/bird_feed_semi"
      }
    },
    {
      "when": {
        "fill": "2"
      },
      "apply": {
        "model": "husbandry:block/bird_feed_full"
      }
    }
  ]
}